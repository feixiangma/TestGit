function updateLink(){
	$(&quot;a&quot;).each(function(){
		var hrefValue = $(this).attr(&apos;href&apos;);
		if(hrefValue){
			if(hrefValue.indexOf(&apos;.do&apos;)&gt;0){
				if(!(hrefValue.indexOf(&apos;.do?&apos;)&gt;0)){
					hrefValue = hrefValue+&quot;?myTime=&quot;+myTime;
					$(this).attr(&apos;href&apos;,hrefValue);
				}
				if(hrefValue.indexOf(&apos;.do?p&apos;)&gt;0){
					hrefValue = hrefValue+&quot;&amp;myTime=&quot;+myTime;
					$(this).attr(&apos;href&apos;,hrefValue);
				}
			}
			
			if(hrefValue.indexOf(&apos;.html&apos;)&gt;0){
				if(!(hrefValue.indexOf(&apos;.do?&apos;)&gt;0)){
					hrefValue = hrefValue+&quot;?myTime=&quot;+myTime;
					$(this).attr(&apos;href&apos;,hrefValue);
				}
			}
			
		}
	});
}


