    function toDecimal2(x) {
        var f = parseFloat(x);
        if (isNaN(f)) {
            return false;
        }
        var f = Math.round(x*100)/100;
        var s = f.toString();
        var rs = s.indexOf(&apos;.&apos;);
        if (rs &lt; 0) {
            rs = s.length;
            s += &apos;.&apos;;
        }
        while (s.length &lt;= rs + 2) {
            s += &apos;0&apos;;
        }
        return s;
    }
