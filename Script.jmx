    function countdownCallback(e) {
        switch (e.type) {
            case &apos;start&apos;:
                ticktack(this);
                break;
            case &apos;timing&apos;:
                e.unit &gt; 0 &amp;&amp; ticktack(this);
                break;
            case &apos;stop&apos;:
                break;
        }

        function ticktack(cd) {
            var des = cd.domElements;
            $(des.hours).text(Number.pad2Str(cd._hours));
            $(des.minutes).text(Number.pad2Str(cd._minutes));
            $(des.seconds).text(Number.pad2Str(cd._seconds));
        }
    }

